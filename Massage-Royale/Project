  <script>
    const btnTherapeutique = document.getElementById('button-therapeutique');

    document
      .getElementById('form-therapeutique')
      .addEventListener('submit', function (event) {
        event.preventDefault();

        btnTherapeutique.value = 'Sending...';

        const serviceID = 'default_service';
        const templateID = 'template_zgkslv3';

        emailjs.sendForm(serviceID, templateID, this).then(
          () => {
            btnTherapeutique.value = 'Send Email';
            alert('Sent!');
          },
          (err) => {
            btnTherapeutique.value = 'Send Email';
            alert(JSON.stringify(err));
          }
        );
      });
  </script>
