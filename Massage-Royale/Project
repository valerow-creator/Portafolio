  <script>
    const btnDetente = document.getElementById('button-d-etente');

    document
      .getElementById('forms')
      .addEventListener('submit', function (event) {
        event.preventDefault();

        btnDetente.value = 'Sending...';

        const serviceID = 'default_service';
        const templateID = 'template_6mqg58v';

        emailjs.sendForm(serviceID, templateID, this).then(
          () => {
            btnDetente.value =
              'Réservation Envoyé. Votre confirmation arrivera bientôt';
            alert('Sent!');
          },
          (err) => {
            btnDetente.value = 'Send Email';
            alert(JSON.stringify(err));
          }
        );
      });
  </script>
